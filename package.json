{
  "name": "skyebridge",
  "version": "0.2.0",
  "description": "Flow diagram generator",
  "keywords": [
    "cli",
    "diagram",
    "generator",
    "flow",
    "transition"
  ],
  "license": "MIT",
  "author": "emsk",
  "files": [
    "src"
  ],
  "main": "src/cli.js",
  "bin": "src/cli.js",
  "repository": "emsk/skyebridge",
  "scripts": {
    "lint": "xo",
    "test": "nyc ava --verbose"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "commander": "^2.20.0",
    "execa": "^2.0.3",
    "html-minifier": "^4.0.0",
    "ora": "^3.4.0",
    "terser": "^4.2.1",
    "vis-network": "^5.3.1"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "nock": "^11.3.1",
    "nyc": "^14.1.1",
    "tmp": "^0.1.0",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
