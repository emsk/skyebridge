{
  "name": "skyebridge",
  "version": "0.1.0",
  "description": "Flow diagram generator",
  "keywords": [
    "cli",
    "diagram",
    "generator",
    "flow",
    "transition"
  ],
  "license": "MIT",
  "author": "emsk",
  "files": [
    "assets"
  ],
  "main": "cli.js",
  "bin": "cli.js",
  "repository": "emsk/skyebridge",
  "scripts": {
    "lint": "xo",
    "test": "nyc ava --verbose"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "html-minifier": "^4.0.0",
    "ora": "^3.4.0",
    "terser": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "execa": "^1.0.0",
    "nyc": "^14.1.1",
    "tmp": "^0.1.0",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  }
}
