{
  "name": "skyebridge",
  "version": "0.2.0",
  "description": "Flow diagram generator",
  "keywords": [
    "cli",
    "diagram",
    "generator",
    "flow",
    "transition"
  ],
  "license": "MIT",
  "author": "emsk",
  "files": [
    "src"
  ],
  "main": "src/cli.js",
  "bin": "src/cli.js",
  "repository": "emsk/skyebridge",
  "scripts": {
    "lint": "xo",
    "test": "nyc ava --verbose"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "commander": "^3.0.1",
    "execa": "^5.0.0",
    "html-minifier": "^4.0.0",
    "ora": "^5.4.0",
    "terser": "^4.8.0",
    "vis-network": "^6.0.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "tmp": "^0.2.1",
    "xo": "^0.39.1"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
